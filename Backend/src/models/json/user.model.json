{
  "table": "users",
  "columns": {
    "username": {
      "type": "VARCHAR",
      "length": 30,
      "nullable": false
    },
    "email": {
      "type": "VARCHAR",
      "length": 100,
      "nullable": false,
      "unique": true,
      "indexed": true
    },
    "password_hash": {
      "type": "VARCHAR",
      "nullable": false
    },
    "is_active": {
      "type": "BOOLEAN",
      "default": true
    },
    "is_verified": {
      "type": "BOOLEAN",
      "default": false
    },
    "created_at": {
      "type": "TIMESTAMP",
      "default": "utcnow"
    },
    "metadata": {
      "type": "JSON",
      "default": {}
    }
  }
}
